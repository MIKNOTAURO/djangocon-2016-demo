[hadjango]
name=hadjango
baseurl=https://hadjango.github.io/rpms/el/7/$basearch
repo_gpgcheck=0
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-hadjango
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300

[hadjango-source]
name=hadjango-source
baseurl=https://hadjango.github.io/rpms/el/7/SRPMS
repo_gpgcheck=0
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-hadjango
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
